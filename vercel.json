{
    "version": 2,
    "builds": [
      {
        "src": "./api/index.js",
        "use": "@vercel/node"
      },
      {
        "src": "./build/**",
        "use": "@vercel/static",
        "config": { "includeFiles": ["build/**"] }
      }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/build/index.js"
        },
        {
            "src": "/",
            "dest": "build/index.html"
          },
          {
            "src": "/(.+)",
            "dest": "build/$1"
          }
    ]
  }